{
  "name": "stylin-with-sass-class-project-files",
  "version": "2.0.0",
  "description": "practice folder for Stylin with Sass class",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "css": "node-sass --output css --source-map true --source-map-contents sass",
    "css:watch": "node-sass --watch --recursive --output css --source-map true --source-map-contents sass",
    "serve": "browser-sync start --server . --files . css/**/*.css css/**/*.css.map images/**/*",
    "prestart": "npm run css",
    "start": "concurrently \"npm run css\" \"npm run serve\""
  },
  "keywords": [
    "sass",
    "class"
  ],
  "author": "tamouse@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.18.11",
    "concurrently": "^3.4.0",
    "node-sass": "^4.5.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gdiminneapolis/html-320-stylin-with-sass-project-files.git"
  },
  "bugs": {
    "url": "https://github.com/gdiminneapolis/html-320-stylin-with-sass-project-files/issues"
  },
  "homepage": "https://github.com/gdiminneapolis/html-320-stylin-with-sass-project-files#readme"
}
